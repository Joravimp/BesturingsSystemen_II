1: 1.c
	gcc -o ./bin/1 1.c
2: 2.c
	gcc -o ./bin/2 2.c
3.1: 3.1.c
	gcc -o ./bin/3.1 3.1.c
3.2: 3.2.c
	gcc -o ./bin/3.2 3.2.c
writestring: writestring.c
	gcc -o ./bin/writestring writestring.c
watchfile: watchfile.c
	gcc -o ./bin/watchfile watchfile.c
watchfiled: watchfiled.cc
	g++ -o ./bin/watchfiled watchfiled.cc
